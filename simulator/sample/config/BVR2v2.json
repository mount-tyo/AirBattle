{
	"Manager":{
		"TimeStep":{"baseTimeStep":0.1,"agentInterval":10},
		"Ruler":"R3BVRRuler01",
		"Assets":{
			"type":"group",
			"Blue":{"type":"broadcast","number":2,
				"names":["Blue1","Blue2"],
				"element":{"type":"alias","alias":"BlueFighter"},
				"overrider":[
					{"type":"alias","alias":"OnEastLine"},
					{"type":"broadcast","number":2,
					"element":{"type":"direct","value":{
						"instanceConfig":{"datalinkName":"BlueDatalink"}
					}}},
					{"type":"group","order":"shuffled","elements":[
						{"type":"direct","value":{"Agent":{"type":"alias","alias":"BlueAgents","instance":"g1","index":0}}},
						{"type":"direct","value":{"Agent":{"type":"alias","alias":"BlueAgents","instance":"g1","index":1}}}
					]}
				]
			},
			"Red":{"type":"broadcast","number":2,
				"names":["Red1","Red2"],
				"element":{"type":"alias","alias":"RedFighter"},
				"overrider":[
					{"type":"alias","alias":"OnWestLine"},
					{"type":"broadcast","number":2,
					"element":{"type":"direct","value":{
						"instanceConfig":{"datalinkName":"RedDatalink"}
					}}},
					{"type":"group","order":"fixed","elements":[
						{"type":"direct","value":{"Agent":{"type":"alias","alias":"RedAgents","instance":"g1","index":0}}},
						{"type":"direct","value":{"Agent":{"type":"alias","alias":"RedAgents","instance":"g1","index":1}}}
					]}
				]
			}
		},
		"CommunicationBuffers":{
			"BlueDatalink":{"participants":["PhysicalAsset:Blue/Blue[0-9]+"]},
			"RedDatalink":{"participants":["PhysicalAsset:Red/Red[0-9]+"]}
		},
		"AssetConfigDispatcher":{
			"BlueFighter":{"type":"PhysicalAsset","model":"R3Fighter00","Agent":{"type":"alias","alias":"Initial"}},
			"RedFighter":{"type":"PhysicalAsset","model":"R3Fighter00","Agent":{"type":"alias","alias":"Initial"}},
			"OnWestLine":{"type":"group","order":"fixed","elements":[
				{"type":"direct","value":{"instanceConfig":{"pos":[-10000,-100000,-10000],"vel":300,"heading":90}}},
				{"type":"direct","value":{"instanceConfig":{"pos":[10000,-100000,-10000],"vel":300,"heading":90}}}
			]},
			"OnEastLine":{"type":"group","order":"fixed","elements":[
				{"type":"direct","value":{"instanceConfig":{"pos":[-10000,100000,-10000],"vel":300,"heading":270}}},
				{"type":"direct","value":{"instanceConfig":{"pos":[10000,100000,-10000],"vel":300,"heading":270}}}
			]}
		}
	}
}
